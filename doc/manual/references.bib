@article{srinivasan_testing_2003,
	title = {Testing parallel random number generators},
	volume = {29},
	issn = {0167-8191},
	url = {http://www.sciencedirect.com/science/article/pii/S0167819102001631},
	doi = {10.1016/S0167-8191(02)00163-1},
	abstract = {Monte Carlo computations are considered easy to parallelize. However, the results can be adversely affected by defects in the parallel pseudorandom number generator used. A parallel pseudorandom number generator must be tested for two types of correlations––(i) intra-stream correlation, as for any sequential generator, and (ii) inter-stream correlation for correlations between random number streams on different processes. Since bounds on these correlations are difficult to prove mathematically, large and thorough empirical tests are necessary. Many of the popular pseudorandom number generators in use today were tested when computational power was much lower, and hence they were evaluated with much smaller test sizes. This paper describes several tests of pseudorandom number generators, both statistical and application-based. We show defects in several popular generators. We describe the implementation of these tests in the {SPRNG} [{ACM} Trans. Math. Software 26 (2000) 436; {SPRNG––scalable} parallel random number generators. {SPRNG} {1.0––http://www.ncsa.uiuc.edu/Apps/SPRNG;} {SPRNG} 2.0––http://sprng.cs.fsu.edu] test suite and also present results for the tests conducted on the {SPRNG} generators. These generators have passed some of the largest empirical random number tests.},
	number = {1},
	urldate = {2012-12-23},
	journal = {Parallel Computing},
	author = {Srinivasan, Ashok and Mascagni, Michael and Ceperley, David},
	month = jan,
	year = {2003},
	keywords = {Parallel algorithms, Parallel random number generators, Random number software, Random number tests},
	pages = {69--94},
	file = {ScienceDirect Snapshot:files/6083/S0167819102001631.html:text/html;Testing_parallel_random_number_generators.pdf:files/6084/Testing_parallel_random_number_generators.pdf:application/pdf}
