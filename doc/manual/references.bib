	publisher = {{ACM}},
	author = {Blelloch, Guy E. and Peng, Richard and Tangwongsan, Kanat},
	year = {2011},
	keywords = {approximation algorithms, facility location, max cover, Parallel algorithms, set cover},
	pages = {23–32},
	file = {Greedy_Parallel_Set_Cover.pdf:files/6205/Greedy_Parallel_Set_Cover.pdf:application/pdf}
},

